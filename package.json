{
  "name": "vue-smooth-dnd",
  "version": "0.3.2",
  "description": "Vue wrappers for smooth-dnd",
  "author": "Kutlu Sahin",
  "homepage": "https://kutlugsahin.github.io/vue-smooth-dnd/",
  "repository": {
    "type": "git",
    "url": "https://github.com/kutlugsahin/vue-smooth-dnd"
  },
  "contributors": [
    {
      "name": "Dave Stewart",
      "url": "https://github.com/davestewart"
    }
  ],
  "keywords": [
    "Vue",
    "Vue.js",
    "sortable",
    "drag and drop",
    "drag&drop",
    "drag",
    "drop",
    "draggable",
    "dnd",
    "smooth-dnd"
  ],
  "main": "dist/vue-smooth-dnd.js",
  "module": "dist/vue-smooth-dnd.esm.js",
  "jsnext:main": "dist/vue-smooth-dnd.esm.js",
  "files": [
    "dist/*"
  ],
  "types": "./dist/main.d.ts",
  "scripts": {
    "dev": "rollup -c build/rollup.js -w",
    "tsc": "tsc --emitDeclarationOnly --outDir dist",
    "build": "yarn run tsc && rollup -c build/rollup.js",
    "demo": "cd demo && npm run dev",
    "test": "jest --watchAll --verbose",
    "unit": "jest --config jest.config.js --coverage",
    "tslint": "tslint -c tslint.json 'src/**/*.tsx?'"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kutlugsahin/vue-smooth-dnd/issues"
  },
  "dependencies": {
    "rollup-plugin-license": "^0.8.1",
    "smooth-dnd": "^0.8.8"
  },
  "devDependencies": {
    "@babel/core": "7.3.4",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/preset-env": "7.3.4",
    "@babel/preset-typescript": "7.3.3",
    "jest": "^23.4.1",
    "rollup": "1.4.0",
    "rollup-plugin-babel": "4.3.2",
    "rollup-plugin-commonjs": "^9.2.1",
    "rollup-plugin-uglify": "6.0.2",
    "typescript": "3.3.3333",
    "tslint": "5.14.0"
  },
  "peerDependencies": {
    "vue": "^2.6.9"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
